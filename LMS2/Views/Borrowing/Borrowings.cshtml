@{
ViewData["Title"] = "Borrowings";
}

<div>
<h1>@ViewData["Title"]</h1>
	
	<ul>
		@if (Model != null && Model.Any())
		{
			foreach (var borrowing in Model)
			{
				<li>Borrow date: @borrowing.BorrowDate | Return date: @borrowing.ReturnDate</li>
			}
		}
		else
		{
			<p>No borrowings found.</p>
		}
	</ul>
</div>
